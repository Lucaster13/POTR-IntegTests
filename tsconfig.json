{
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["es5", "dom"],
        "types": ["cypress", "node"],
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noEmit": true,
        "baseUrl": "./src"
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"]
}
